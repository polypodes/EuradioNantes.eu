<section class="content-main">
  {{ wo_render_breadcrumbs() }}

  <h1 class="content-title">Recherche</h1>

  <h2>Résultats de la recherche « {{ app.request.query.get('search') }} »</h2>

  <gcse:searchresults-only></gcse:searchresults-only>

</section>

<aside class="content-aside">
  <div class="content-aside-inner">
    {{ render(controller('SonataNewsBundle:Post:getAsidePosts')) }}

    {{ render(controller('SiteBundle:Newsletter:subscribe')) }}
  </div>
</aside>

<script>
  (function() {
    var cx = '{{ google_search_cx }}';
    var gcse = document.createElement('script'); gcse.type = 'text/javascript'; gcse.async = true;
    gcse.src = (document.location.protocol == 'https:' ? 'https:' : 'http:') +
        '//www.google.fr/cse/cse.js?cx=' + cx;
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(gcse, s);
     })();
</script>
